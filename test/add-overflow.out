@pow
	op: const		dest: out		type: int		value: 1	
	op: const		dest: one		type: int		value: 1	
.loop
	op: lt   		dest: end		type: bool		args: ["exp", "one"]	
	op: br   		args: ["end"]		labels: ["ret", "body"]
.body
	op: mul  		dest: out		type: int		args: ["out", "base"]	
	op: sub  		dest: exp		type: int		args: ["exp", "one"]	
	op: jmp  		labels: ["loop"]
.ret
	op: ret  		args: ["out"]	
@main
	op: const		dest: one		type: int		value: 1	
	op: const		dest: two		type: int		value: 2	
	op: const		dest: sixtytwo		type: int		value: 62	
	op: call 		dest: half		type: int		args: ["two", "sixtytwo"]		funcs: ["pow"]
	op: print		args: ["half"]	
	op: sub  		dest: smaller		type: int		args: ["half", "one"]	
	op: add  		dest: maxint		type: int		args: ["half", "smaller"]	
	op: print		args: ["maxint"]	
	op: add  		dest: overflow		type: int		args: ["maxint", "one"]	
	op: print		args: ["overflow"]	
